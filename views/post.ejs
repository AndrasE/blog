<%- include("partials/header"); -%>


  <div class="about-container">
    <div class="row">
        <div class="col-sm-12">
          <div class="card">
            <div class="card-body">
              <h5 class="card-title">
                <%=title%>
              </h5>
              <p class="card-text">
                <%=content%>
              </p>
              <form class="" action="/delete" method="post">
                <button class="btn btn-primary" type="submit" name="button" value="<%=postId%>" onClick="this.form.submit()">Delete</button>
              </form>
            </div>
          </div>
        </div>
    </div>
  </div>


<%- include("partials/footer"); -%>
